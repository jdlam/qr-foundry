{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-image",
    "clipboard-manager:allow-read-image",
    "fs:default",
    "fs:allow-write",
    "fs:allow-read"
  ]
}
